{
    "name": "dwoodard/schemagen",
    "description": "A package to parse custom schema syntax and generate Laravel and Vue files.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Woodard",
            "email": "dustin.woodard@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "twig/twig": "^3.0",
        "symfony/console": "^5.0 || ^6.0 || ^7.0",
        "doctrine/lexer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SchemaGen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SchemaGen\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SchemaGen\\SchemaGenServiceProvider"
            ]
        }
    }
}